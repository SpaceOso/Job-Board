{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb",
        "tslint-react"
    ],
    "jsRules": {},
    "rulesDirectory": ["tslint-consistent-codestyle"],
    "rules": {
        "no-console": [false],
        "import-name": true,
        "member-ordering": [false],
        "max-line-length": [false],
        "interface-name": [false],
        "jsx-boolean-value": false,
        "member-access": [true, "no-public"]
    }
}